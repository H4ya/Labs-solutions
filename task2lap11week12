#include<iostream>
#include <string>
#include <cstring>
using namespace std;
int main() {
    
    string inpt;
    cin>>inpt;
    for(int i=0;i<inpt.size();i++){
        if(islower(inpt.at(i)))
            cout<<char(toupper(inpt.at(i)));
        else
            cout<<char(tolower(inpt.at(i)));
    }
    
    return 0;
}
