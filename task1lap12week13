#include<iostream>
#include <string>
#include <cstring>
using namespace std;
int main() {
    const int SIZE=4;
    int arr[]={1,5,7,-1};
    int sum=6,count=0;
    for(int i=0;i<SIZE;i++){
        for(int j=i+1;j<SIZE;j++){
            if(arr[i]+arr[j]==sum)
                count++;
        }
    }
    cout<<count;
    
    
    return 0;
}
