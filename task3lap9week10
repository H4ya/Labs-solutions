//with string!

#include <iostream>
#include <cstring>
#include <string>
using namespace std;
int main(){
    const string FAV="water melon";
    string x;
    while(true){
    getline(cin,x);
    if(x!=FAV){
        cout<<"Guess!!\n";
        continue;}
    break;
    }
    cout<<"Correct!";
return 0;
}
