#include<iostream>
#include <string>
#include <cstring>
using namespace std;
int main() {
    string num;
    cout<<"Enter phone num ";
    cin>> num;
    cout<<"Country: ";
    switch(char(num.at(3))){
    case '6':
        cout<<"KSA";
        break;
    case '2':
        cout<<"Jordan";
        break;
    case '7':
        cout<<"UAE";
        break;
    
    default:
        cout<<"Unknown";
    }
    return 0;

}
